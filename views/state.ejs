<%-    include("partials/header")   -%>
<h1 >INDIA State Wise Covid-19 Cases</h1>

<% console.log(indiastate)%>

<table class="table  col-lg-4 table-hover table-striped table-bordered">
<thead class="thead-dark">
<tr>
<th>States Names</th>
<th>Active Cases</th>
<th>Confirmed Cases</th>
<th>Recovered Cases</th>
<th>Deaths</th>
</tr>
</thead>
<%  for(var i=0;i<indiastate.length;i++){%>
<tbody>

<tr>

<td><%= indiastate[i].state %></td>
<td><%= indiastate[i].active %></td>
<td><%= indiastate[i].confirmed %></td>
<td><%= indiastate[i].recovered %></td>
<td><%= indiastate[i].deaths %></td>

</tr>

</tbody>
<%}%>
</table>





















<%-    include("partials/footer")   -%>s